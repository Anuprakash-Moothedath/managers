<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    
    <parent>
        <groupId>dev.galasa</groupId>
        <artifactId>galasa-managers-testingtools-parent</artifactId>
        <version>0.6.0-SNAPSHOT</version>
    </parent>
    
    <artifactId>dev.galasa.selenium.manager</artifactId>
    <version>0.6.0-SNAPSHOT</version>
    <packaging>bundle</packaging>

    <name>Galasa Selenium Manager</name>
  
    <dependencies>
		<dependency>
			<groupId>temp</groupId>
			<artifactId>selenium-java</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/selenium-java-3.141.59.jar</systemPath>
		</dependency>
        <dependency>
			<groupId>temp</groupId>
			<artifactId>selenium-api</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/selenium-api-3.141.59.jar</systemPath>
		</dependency>
        <dependency>
			<groupId>temp</groupId>
			<artifactId>selenium-remote-driver</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/selenium-remote-driver-3.141.59.jar</systemPath>
		</dependency>
        <dependency>
			<groupId>temp</groupId>
			<artifactId>selenium-support</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/selenium-support-3.141.59.jar</systemPath>
		</dependency>
        <dependency>
			<groupId>temp</groupId>
			<artifactId>selenium-firefox-driver</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/selenium-firefox-driver-3.141.59.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>temp</groupId>
			<artifactId>selenium-chrome-driver</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/selenium-chrome-driver-3.141.59.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>temp</groupId>
			<artifactId>selenium-safari-driver</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/selenium-safari-driver-3.141.59.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>temp</groupId>
			<artifactId>selenium-opera-driver</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/selenium-opera-driver-3.141.59.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>temp</groupId>
			<artifactId>selenium-edge-driver</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/selenium-edge-driver-3.141.59.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>temp</groupId>
			<artifactId>selenium-ie-driver</artifactId>
			<version>3.141.59</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/selenium-ie-driver-3.141.59.jar</systemPath>
		</dependency>
        <dependency>
			<groupId>temp</groupId>
			<artifactId>guava</artifactId>
			<version>25.0-jre</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/guava-25.0-jre.jar</systemPath>
		</dependency>
        <dependency>
			<groupId>temp</groupId>
			<artifactId>okhttp</artifactId>
			<version>3.11.0</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/okhttp-3.11.0.jar</systemPath>
		</dependency>
        <dependency>
			<groupId>temp</groupId>
			<artifactId>okio</artifactId>
			<version>1.14.0</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/okio-1.14.0.jar</systemPath>
		</dependency>
        <dependency>
			<groupId>temp</groupId>
			<artifactId>byte-buddy</artifactId>
			<version>1.8.15</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/byte-buddy-1.8.15.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>temp</groupId>
			<artifactId>commons-exec</artifactId>
			<version>1.3</version>
			<scope>system</scope>
			<systemPath>${project.basedir}/jars/commons-exec-1.3.jar</systemPath>
		</dependency>
		<dependency>
			<groupId>commons-io</groupId>
			<artifactId>commons-io</artifactId>
			<version>2.6</version>
			<scope>provided</scope>
		</dependency>
        <dependency>
            <groupId>dev.galasa</groupId>
            <artifactId>dev.galasa.framework</artifactId>
            <version>0.6.0-SNAPSHOT</version>
			<scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>dev.galasa</groupId>
            <artifactId>dev.galasa</artifactId>
            <version>0.6.0-SNAPSHOT</version>
			<scope>provided</scope>
        </dependency>
    </dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.felix</groupId>
				<artifactId>maven-bundle-plugin</artifactId>
				<configuration>
					<instructions>
						<Embed-Dependency>*;scope=compile</Embed-Dependency>
						<Embed-Dependency>*;scope=compile</Embed-Dependency>
						<Embed-Transitive>true</Embed-Transitive>
						<Import-Package>
							dev.galasa,
							dev.galasa.framework,
							dev.galasa.framework.spi,
							dev.galasa.framework.spi.cps,
							dev.galasa.selenium,
							org.openqa.selenium,
							org.openqa.selenium.support.ui,
							org.openqa.selenium.firefox.internal,
							org.openqa.selenium.*,
							org.apache.commons,
							org.apache.commons.exec,
							org.apache.commons.*,
							com.google.common.*,
							okhttp3,
							okhttp3.internal,
							okhttp3.internal.*,
							okio,
							javax.net.*
						</Import-Package>
						<Export-Package>dev.galasa.selenium,
							org.openqa.selenium,
							org.openqa.selenium.support.ui,
							org.openqa.selenium.firefox.internal,
							org.openqa.selenium.*,
							org.apache.commons,
							org.apache.commons.exec,
							!org.apache.commons.io.monitor,
							org.apache.commons.*,
							com.google.common.*,
							okhttp3,
							okhttp3.internal,
							okhttp3.internal.*,
							okio
						</Export-Package>
					</instructions>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>